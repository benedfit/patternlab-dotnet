<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PatternLabStyleguide" path="styleguide/*" verb="*"
        type="System.Web.StaticFileHandler" />
      <add name="PatternLabPatterns" path="patterns/*/*.html" verb="*"
        type="System.Web.StaticFileHandler" />
    </handlers>
  </system.webServer>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- Configuration Options for Pattern Lab -->
    <add key="PatternLabAutoReloadNav" value="true" />
    <add key="PatternLabAuthReloadPort" value="8002" />
    <add key="PatternLabFollowNav" value="true" />
    <add key="PatternLabFollowPort" value="8003" />
    <add key="PatternLabQRCodeGeneratorOn" value="false" />
    <add key="PatternLabXIPHostname" value="http://patternlab.*.xip.io" />
    <add key="PatternLabIshMinimum" value="240" />
    <add key="PatternLabIshMaximum" value="2600" />
    <add key="PatternLabIshControlsHide" value="hay" />
    <add key="PatternLabPatternStates" value="inprogress,inreview,complete" />
    <add key="PatternLabStyleGuideExcludes" value="" />
    <add key="PatternLabCacheBusterOn" value="true" />
    <add key="PatternLabCssEnabled" value="false" />
    <!-- /Configuration Options for Pattern Lab -->
  </appSettings>
  <system.web>
    <httpRuntime targetFramework="4.5" />
    <compilation debug="true" targetFramework="4.5" />
  </system.web>
</configuration>